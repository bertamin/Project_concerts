<h1> Editing concert </h1>
<%= form_for :concert, url: concert_path(@concert), method: :patch do |f| %>
	<p>
		<%= f.label :band_name %><br>
		<%= f.text_field :band_name %>
	</p>
	<p>
		<%= f.label :venue %><br>
		<%= f.text_field :venue %>
	</p>
	<p>
		<%= f.label :city %><br>
		<%= f.text_field :city %>
	</p>
	<p>
		<%= f.label :date %><br>
		<%= f.text_field :date %>
	</p>
	<p>
		<%= f.label :price %><br>
		<%= f.text_field :price %>
	</p>
	<p>
		<%= f.label :description %><br>
		<%= f.text_area :description %>
	</p>
	<p>
		<%= f.submit %>
	</p>
<% end %>
<%= link_to "Concerts page", concerts_path %>